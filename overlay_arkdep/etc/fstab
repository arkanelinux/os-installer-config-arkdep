LABEL=arkane_root		/home								btrfs     	rw,relatime,subvol=arkdep/shared/home,compress=zstd	0 0
LABEL=arkane_root		/root								btrfs     	rw,relatime,subvol=arkdep/shared/root,compress=zstd	0 0
LABEL=arkane_root		/arkdep							btrfs     	rw,relatime,subvol=arkdep,compress=zstd	0 0
LABEL=arkane_root		/var/lib/flatpak		btrfs     	rw,relatime,subvol=arkdep/shared/flatpak,compress=zstd 0 0
LABEL=arkane_boot		/boot								vfat      	rw,relatime,fmask=0022,dmask=0022,codepage=437,iocharset=ascii,shortname=mixed,utf8,errors=remount-ro	0 2

/arkdep/shared/swapfile				none      	swap      	defaults  	0 0
